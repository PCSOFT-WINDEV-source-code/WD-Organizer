     code : |1-
      PROCEDURE DateTimeToString(LOCAL dtDateTime is datetime, LOCAL sSeparator is string = " to ")
      
      	sDateTime is string
      	
      	// Conversion
      	sDateTime = DateToString(dtDateTime..Date) + [" "] + sSeparator + [" "] + TimeToString(dtDateTime..Time, "HH:MM")	
      	
      RESULT sDateTime
