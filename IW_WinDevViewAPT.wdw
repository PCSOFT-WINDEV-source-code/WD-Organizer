        code : |1-
         PROCEDURE IW_WinDevViewAPT (AnAppointment is an Appointment)
         
         sImage is string = AnAppointment..Image
         
         STC_BCKGRD..BackgroundColor			= AnAppointment..BackgroundColor
         STC_DETAILS 						= AnAppointment..Content
         STC_APTTITLE 					= AnAppointment..Title
         IMG_PICTO1..Value 				= ExtractString(sImage, 1, CR)
         IMG_PICTO2..Value 				= Replace(ExtractString(sImage, 2, CR), EOT, "")
         IMG_PICTO3..Value 				= Replace(ExtractString(sImage, 3, CR), EOT, "")
         IMG_IMPORTANCE..Visible			= (AnAppointment..Importance <> 0)
         IMG_REPETITION..Visible			= AnAppointment..WithRepetition
         
         // If the window is too small
         IF MySelf..Height < 32 THEN
         	
         	// Moves the title according to the pictos
         	STC_APTTITLE..X = STC_APTTITLE..XInitial + (18 * ((IMG_PICTO1 <> "" ? 1 ELSE 0) + (IMG_PICTO2 <> "" ? 1 ELSE 0) + (IMG_PICTO3 <> "" ? 1 ELSE 0)))
         	
         	STC_APTTITLE = STC_APTTITLE + gFontBold(False) + ["; "] + AnAppointment..Content
         END
         
         // If the lightness of the color is low, switch the text to white
         if ColorLightness(AnAppointment..BackgroundColor) < 60 then
         	STC_DETAILS..Color 	= White
         	STC_APTTITLE..Color 	= White
         ELSE
         	STC_DETAILS..Color 	= iDefaultColor
         	STC_APTTITLE..Color 	= iDefaultColor
         END
