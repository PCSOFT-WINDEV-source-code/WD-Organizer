      // Summary: Gives focus to the organizer
      PROCEDURE OrganizerGiveFocus(Organizer, LOCAL nMouseXPos is int = MouseXPos, LOCAL nMouseYPos is int = MouseYPos)
      
      	AppointmentClick is an Appointment <- Null
      	nSubscript		   is int
      
      	// Gives focus
      	SetFocus(Organizer)
      	
      	nSubscript = OrganizerInfoXY(Organizer, oriAppointment, nMouseXPos, nMouseYPos)
      	IF nSubscript > 0 THEN
      
      		// Simulates a click if an event is hovered, to select it
      		EmulateMouse(Organizer, emLeftClick, nMouseXPos, nMouseYPos)
      		
      		// Retrieves the selected appointment
      		AppointmentClick <- Organizer[nSubscript]
      	END	
      	
      	// If we have a copied appointment
      	IF AppointmentClipboard <> Null THEN
      		
      		// If no period was defined
      		IF AppointmentClick = Null AND SelectedPeriod.StartDate = "00000000000000000" THEN
      		
      			// Calculates the duration of the event
      			duDuration	is duration = (AppointmentClipboard..EndDate - AppointmentClipboard..StartDate)			
      			
      			// Retrieves the start period hovered
      			dtDateTime is datetime = OrganizerInfoXY(Organizer, oriDateTime, nMouseXPos, nMouseYPos)
      			
      			// Defines the beginning of the period
      			Organizer..StartDate = dtDateTime
      			
      			// Assigns the same duration as the event				
      			dtDateTime = duDuration + dtDateTime
      				
      			// Defines the end of the period		
      			Organizer..EndDate = dtDateTime
      		END
      	ELSE
      		
      		// If no period is selected
      		IF AppointmentClick = Null _AND_ Organizer..StartDate = "" THEN
      			
      			// Simulates a click to select a period
      			EmulateMouse(Organizer, emLeftClick, nMouseXPos, nMouseYPos)
      		END
      	END
