      // Summary: Run when changing the zoom of the organizer
      PROCEDURE OnOrganizerZoom()
      
      	dDate is date
      	dDate = OrganizerPosition(ORG_ORGANIZER)
      	
      	AnimationPrepare(MyWindow)
      	
      	// According to the initial zoom of the organizer
      	SWITCH OrganizerZoom(ORG_ORGANIZER)
      	
      		// -> full week
      		CASE orgzWeek
      			
      			// Makes the week picker visible
      			RADIO_WEEKTYPE..Visible = True
      			STC_WEEKNUM..Visible = True
      			RADIO_WEEKTYPE = 1
      			
      			STC_WEEKNUM = "Week " + DateToWeekNumber(dDate) + "/" + dDate..Year
      			
      			// Menu options
      			OPT_Display.OPT_Day..Checked = False
      			OPT_Display.OPT_Month..Checked = False
      			OPT_Display.OPT_Week.OPT_Working_week..Checked = False
      			OPT_Display.OPT_Week.OPT_Entire_week..Checked = True
      			
      			BTN_Day..Pushed = False
      			BTN_Month..Pushed = False
      			BTN_Week..Pushed = True
      			
      			// Restores the selected period
      			ORG_ORGANIZER..StartDate = SelectedPeriod.StartDate
      			ORG_ORGANIZER..EndDate = SelectedPeriod.EndDate
      			
      			
      		// -> week on 5 days
      		CASE orgzWeekOn5
      			
      			// Makes the week picker visible
      			RADIO_WEEKTYPE..Visible = True
      			STC_WEEKNUM..Visible = True
      			RADIO_WEEKTYPE = 2
      
      			STC_WEEKNUM = "Week " + DateToWeekNumber(dDate) + "/" + dDate..Year
      
      			// Menu options
      			OPT_Display.OPT_Day..Checked = False
      			OPT_Display.OPT_Month..Checked = False
      			OPT_Display.OPT_Week.OPT_Working_week..Checked = True
      			OPT_Display.OPT_Week.OPT_Entire_week..Checked = False
      			
      			
      			BTN_Day..Pushed = False
      			BTN_Month..Pushed = False
      			BTN_Week..Pushed = True
      			
      			// Restores the selected period
      			ORG_ORGANIZER..StartDate = SelectedPeriod.StartDate
      			ORG_ORGANIZER..EndDate = SelectedPeriod.EndDate
      			
      			
      		// -> day
      		CASE orgzDay
      			
      			// Hides the week picker
      			RADIO_WEEKTYPE..Visible = False
      			STC_WEEKNUM..Visible = False
      			
      			// Menu options
      			OPT_Display.OPT_Day..Checked = True
      			OPT_Display.OPT_Month..Checked = False
      			OPT_Display.OPT_Week.OPT_Working_week..Checked = False
      			OPT_Display.OPT_Week.OPT_Entire_week..Checked = False
      			
      			
      			BTN_Day..Pushed = True
      			BTN_Month..Pushed = False
      			BTN_Week..Pushed = False
      			
      			// Restores the selected period
      			ORG_ORGANIZER..StartDate = SelectedPeriod.StartDate
      			ORG_ORGANIZER..EndDate = SelectedPeriod.EndDate
      			
      		// -> month
      		CASE orgzMonth
      			
      			// Hides the week picker
      			RADIO_WEEKTYPE..Visible = False
      			
      			// Menu options
      			OPT_Display.OPT_Day..Checked = False
      			OPT_Display.OPT_Month..Checked = True
      			OPT_Display.OPT_Week.OPT_Working_week..Checked = False
      			OPT_Display.OPT_Week.OPT_Entire_week..Checked = False
      						
      			BTN_Day..Pushed = False
      			BTN_Month..Pushed = True
      			BTN_Week..Pushed = False
      			
      			// Disables the selected period
      			ORG_ORGANIZER..StartDate = ""
      			ORG_ORGANIZER..EndDate = ""
      	END
      
      	AnimationPlay(animFadeIn)
