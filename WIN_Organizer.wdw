      // Summary: Synchronizes the new events
      PROCEDURE SynchronizeEventAdditions()
      
      	ArrayOfArray is Structure
      		Elements is array of int
      	END
      	
      	aaOutlookAdditions 	is ASSOCIATIVE ARRAY (*, *, wlInt) of ArrayOfArray 
      	aaNotesAdditions 		is ASSOCIATIVE ARRAY (*, *, wlInt) of ArrayOfArray 
      	aaGoogleAdditions		is ASSOCIATIVE ARRAY (*, *, wlInt) of ArrayOfArray 
      	AnArrayOfArray	is ArrayOfArray
      	
      	// Disables the triggers temporarily
      	HDeactivateTrigger()
      	
      	// Messages
      	WIN_SynchroInfo.Process(1, "Synchronizing the organizers...", "Gathering the added events...")
      	
      	// Finds the events linked to a Google calendar but for which we have no Google identifier
      	FOR EACH OrganizerEvent WHERE "GoogleCalendarID <> 0 AND GoogleIdentifier = ''"
      		
      		// Retrieves the list of additions
      		IF aaGoogleAdditions[OrganizerEvent.GoogleCalendarID] = Null THEN
      			VariableReset(AnArrayOfArray)
      		ELSE
      			AnArrayOfArray = aaGoogleAdditions[OrganizerEvent.GoogleCalendarID]
      		END
      		
      		// Adds the event to the list
      		Add(AnArrayOfArray:Elements, OrganizerEvent.OrganizerEventID)
      		
      		// Keeps the new list
      		aaGoogleAdditions[OrganizerEvent.GoogleCalendarID] = AnArrayOfArray
      		
      		// Erases the history of this event
      		FOR EACH OrganizerEventHistory WHERE OrganizerEventID = OrganizerEvent.OrganizerEventID
      			HDelete(OrganizerEventHistory)
      		END
      	END
      	
      	// Searches for the events linked to an Outlook calendar but for which we have no Outlook identifier
      	FOR EACH OrganizerEvent WHERE "OutlookCalendarId <> 0 AND OutlookIdentifier = ''"
      		
      		// Retrieves the list of additions
      		IF aaOutlookAdditions[OrganizerEvent.OutlookCalendarID] = Null THEN
      			VariableReset(AnArrayOfArray)
      		ELSE
      			AnArrayOfArray = aaOutlookAdditions[OrganizerEvent.OutlookCalendarID]
      		END
      		
      		// Adds the event to the list
      		Add(AnArrayOfArray:Elements, OrganizerEvent.OrganizerEventID)
      		
      		// Keeps the new list
      		aaOutlookAdditions[OrganizerEvent.OutlookCalendarID] = AnArrayOfArray
      		
      		// Erases the history of this event
      		FOR EACH OrganizerEventHistory WHERE OrganizerEventID = OrganizerEvent.OrganizerEventID
      			HDelete(OrganizerEventHistory)
      		END
      	END
      	
      	// Searches for the events linked to a Notes calendar but for which we have no Notes identifier
      	FOR EACH OrganizerEvent WHERE "NotesCalendarID <> 0 AND NotesIdentifier = ''"
      		
      		// Retrieves the list of additions
      		IF aaNotesAdditions[OrganizerEvent.NotesCalendarID] = Null THEN
      			VariableReset(AnArrayOfArray)
      		ELSE
      			AnArrayOfArray = aaNotesAdditions[OrganizerEvent.NotesCalendarID]
      		END
      		
      		// Adds the event to the list
      		Add(AnArrayOfArray:Elements, OrganizerEvent.OrganizerEventID)
      		
      		// Keeps the new list
      		aaNotesAdditions[OrganizerEvent.NotesCalendarID] = AnArrayOfArray
      		
      		// Erases the history of this event
      		FOR EACH OrganizerEventHistory WHERE OrganizerEventID = OrganizerEvent.OrganizerEventID
      			HDelete(OrganizerEventHistory)
      		END
      	END
      	
      	// Messages
      	WIN_SynchroInfo.Process(1, "Synchronizing the organizers...", "Exporting the added events toward the Outlook calendars..")
      	
      	FOR EACH AnArrayOfArray, nOrganizerID of aaOutlookAdditions
      		OutlookAddMultipleEvents(nOrganizerID, AnArrayOfArray:Elements)	
      	END
      	
      	// Messages
      	WIN_SynchroInfo.Process(1, "Synchronizing the organizers...", "Exporting the added events toward the Lotus calendars..")
      	
      	FOR EACH AnArrayOfArray, nOrganizerID of aaNotesAdditions
      		NotesAddMultipleEvents(nOrganizerID, AnArrayOfArray:Elements)	
      	END
      	
      	// Messages
      	WIN_SynchroInfo.Process(1, "Synchronizing the organizers...", "Exporting the added events toward the Google calendars..")
      	
      	FOR EACH AnArrayOfArray, nOrganizerID of aaGoogleAdditions
      		GoogleAddMultipleEvents(nOrganizerID, AnArrayOfArray:Elements)
      	END
