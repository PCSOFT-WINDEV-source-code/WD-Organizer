      // Summary: Run when linking an event to an external organizer
      PROCEDURE OnEventLink(LOCAL nSource is int, LOCAL nOrganizerID is int)
      
      	// If an appointment was selected
      	IF MySelectedAppointment <> Null THEN
      
      		// Searches for the appointment
      		HReadSeekFirst(OrganizerEvent, OrganizerEventID, MySelectedAppointment..ID)
      		IF HFound(OrganizerEvent) THEN
      
      			// According to the organizer
      			SWITCH nSource
      				
      				// Outlook
      				CASE fromOutlook
      					
      					// Cancels the link
      					IF nOrganizerID = -1 THEN
      						
      						// Deletes the event from the Outlook calendar
      						OutlookDeleteEvent(OrganizerEvent.OutlookCalendarID, OrganizerEvent.OutlookIdentifier)
      						
      						OrganizerEvent.OutlookCalendarID = 0
      						OrganizerEvent.OutlookIdentifier = ""						
      					ELSE
      						OrganizerEvent.OutlookCalendarID = nOrganizerID
      					END
      					
      				// Notes
      				CASE fromNotes
      					
      					// Cancels the link
      					IF nOrganizerID = -1 THEN
      						
      						// Deletes the event from the Lotus calendar
      						NotesDeleteEvent(OrganizerEvent.NotesCalendarID, OrganizerEvent.NotesIdentifier)
      						
      						OrganizerEvent.NotesCalendarID = 0
      						OrganizerEvent.NotesIdentifier = ""
      					ELSE
      						OrganizerEvent.NotesCalendarID = nOrganizerID
      					END
      					
      				// Google
      				CASE fromGoogle
      					
      					// Cancels the link
      					IF nOrganizerID = -1 THEN
      						
      						// Deletes the event from the Google calendar
      						GoogleDeleteEvent(OrganizerEvent.GoogleCalendarID, OrganizerEvent.GoogleIdentifier)
      												
      						OrganizerEvent.GoogleCalendarID = 0
      						OrganizerEvent.GoogleIdentifier = ""
      					ELSE
      						OrganizerEvent.GoogleCalendarID = nOrganizerID
      					END
      			END
      			
      			// Saves the modifications
      			HModify(OrganizerEvent)
      		END
      		
      		MySelectedAppointment <- Null
      				
      		// Refreshes the events
      		DisplayEvents()
      		
      		// Re-selects the appointment
      		OrganizerSelectPlus(ORG_ORGANIZER, AppointmentSubscript)
      	END
