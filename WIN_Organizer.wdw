      // Summary: Displays the selected period
      PROCEDURE CalendarPeriod(dDate is date)
      
      	// Re-selects the date, this is used to cancel the selection in the calendar
      	CAL_CALENDAR = dDate
      	
      	// According to the zoom of the organizer
      	SWITCH OrganizerZoom(ORG_ORGANIZER)
      		
      		// Daily
      		CASE orgzDay
      			CalendarSelectPlus(CAL_CALENDAR, dDate)				
      		
      		// Monthly
      		CASE orgzMonth
      			
      			nMonth is int = dDate..Month
      			dDate..Day = 1
      			
      			// Selects the period
      			WHILE dDate..Month = nMonth			
      				CalendarSelectPlus(CAL_CALENDAR, dDate)	
      				dDate..Day ++
      			END		
      			
      		// Weekly
      		CASE orgzWeek
      			
      			// Week starting on Monday
      			dDate..Day -= (DateToDay(dDate) - 1)
      			
      			// Selects the period
      			FOR i = 1 _TO_ 7
      				CalendarSelectPlus(CAL_CALENDAR, dDate)	
      				dDate..Day ++
      			END
      			
      		// Weekly (working week)
      		CASE orgzWeekOn5
      			
      			// Week starting on Monday
      			dDate..Day -= (DateToDay(dDate) - 1)
      			
      			// Selects the period
      			FOR i = 1 _TO_ 5
      				CalendarSelectPlus(CAL_CALENDAR, dDate)	
      				dDate..Day ++
      			END
      			
      	END
