      // Summary: Synchronizing the events with the external organizers
      PROCEDURE SynchronizeEvents()
      
      	// Opens the progress window
      	OpenChild(WIN_SynchroInfo)
      	
      	// Disable the DDW temporarily to avoid the flickering effects
      	// with the conflict window
      	WinActivateDDW(WIN_Organizer, False)
      	WinActivateDDW(WIN_SynchroInfo, False)
      	
      	// Bound of the progress bar
      	WIN_SynchroInfo.PROGBAR_PROCESS..MaxValue = 3
      	
      	// Messages
      	WIN_SynchroInfo.Process(0, "Synchronizing the organizers...")
      
      	// Start to synchronize the deletions
      	SynchronizeEventDeletions()
      	
      	// Messages
      	WIN_SynchroInfo.Process(1, "Synchronizing the organizers...")
      	
      	// Then, synchronize the new links
      	SynchronizeEventAdditions()	
      	
      	// Messages
      	WIN_SynchroInfo.Process(2, "Synchronizing the organizers...")
      
      	// Finally, synchronize the modifications
      	SynchronizeEventModifications()
      	
      	// Messages
      	WIN_SynchroInfo.Process(3, "Done")
      	
      	Close(WIN_SynchroInfo)
      	
      	// Re-enables the DDW
      	WinActivateDDW(WIN_Organizer, True)
