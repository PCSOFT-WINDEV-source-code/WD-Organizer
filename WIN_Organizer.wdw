      // Summary: Run when deleting an event
      PROCEDURE OnDeleteEvent(MyAppointment is an Appointment)
      
      	sListExternalOrganizer is string
      
      	// Disables the triggers temporarily
      	HDeactivateTrigger()
      
      	// Searches for the event in the database
      	HReadSeekFirst(OrganizerEvent, OrganizerEventID, MyAppointment..ID)
      	dbgAssert(HFound(OrganizerEvent))
      	
      	IF HFound(OrganizerEvent) THEN
      		
      		// If the event is linked to an external organizer
      		IF OrganizerEvent.GoogleCalendarID <> 0 _OR_ OrganizerEvent.NotesCalendarID <> 0 _OR_ OrganizerEvent.OutlookCalendarID <> 0 THEN
      			
      			// If the event is linked to a Google calendar
      			IF OrganizerEvent.GoogleCalendarID <> 0 THEN
      				
      				// Searches for the organizer
      				HReadSeekFirst(GoogleCalendar, GoogleCalendarID, OrganizerEvent.GoogleCalendarID)
      				IF HFound(GoogleCalendar) THEN
      					
      					// Fills the list of external calendars
      					sListExternalOrganizer += [CR] + "- " + GoogleCalendar.Name + " (Google)"
      				END
      			END
      			
      			// If the event is linked to an Outlook calendar
      			IF OrganizerEvent.OutlookCalendarID <> 0 THEN
      				
      				// Searches for the organizer
      				HReadSeekFirst(OutlookCalendar, OutlookCalendarID, OrganizerEvent.OutlookCalendarID)
      				IF HFound(OutlookCalendar) THEN
      					
      					// Fills the list of external calendars
      					sListExternalOrganizer += [CR] + "- " + OutlookCalendar.Name + " (Outlook)"
      				END
      			END
      			
      			// If the event is linked to a Notes calendar
      			IF OrganizerEvent.NotesCalendarID <> 0 THEN
      				
      				// Searches for the organizer
      				HReadSeekFirst(NotesCalendar, NotesCalendarID, OrganizerEvent.NotesCalendarID)
      				IF HFound(NotesCalendar) THEN
      					
      					// Fills the list of external calendars
      					sListExternalOrganizer += [CR] + "- " + NotesCalendar.Name + " (Lotus)"
      				END
      			END
      			
      			//1: &Delete
      			//2: Do&n't delete
      			SWITCH Dialog(<§?1B03C0D9005353F1§>, sListExternalOrganizer)
      				// Do&n't delete
      				CASE 2
      					ReturnToCapture()			
      			END
      			
      			// Flags the event as deleted
      			OrganizerEvent.IsDeleted = True		
      			
      			// Saves the modifications
      			HModify(OrganizerEvent)
      		ELSE
      			
      			// Don't ask for confirmation
      			// Delete directly
      			HDelete(OrganizerEvent)
      		END
      	END
      	
      	// Re-enables the triggers
      	HActivateTrigger()
      
      	// No selected appointment
      	MySelectedAppointment <- Null
      	ModeAppointmentEdition = 0
      	
      	SelectedPeriod.StartDate 	= ""
      	SelectedPeriod.EndDate 	= ""
