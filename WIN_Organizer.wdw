      // Summary: Run when selecting a category
      PROCEDURE OnCategorySelection()
      
      	// Reset the list of selected categories
      	ListCategories = ""
      
      	FOR ALL ROW of TABLE_CATEGORIES
      		IF COL_Selection THEN
      			ListCategories += [";"] + COL_OrganizerCategory
      		END
      	END
      	
      	// Update the appointments
      	DisplayEvents()
