      // Summary: Run when selecting a menu option for the classification
      PROCEDURE OnEventClassification(LOCAL nCategoryID is int)
      
      	// If an appointment was selected
      	IF MySelectedAppointment <> Null THEN
      
      		// Searches for the event
      		HReadSeekFirst(OrganizerEvent, OrganizerEventID, MySelectedAppointment..ID)
      		IF HFound(OrganizerEvent) THEN
      			
      			IF nCategoryID > 0 THEN
      			
      				// Searches for the category assigned
      				HReadSeekFirst(OrganizerCategory, OrganizerCategory, nCategoryID)
      				IF HFound(OrganizerCategory) THEN
      			
      					// Assigns the category
      					OrganizerEvent.CategoryID = nCategoryID
      
      					// Applies the category and the color of the category
      					MySelectedAppointment..Category 	= OrganizerCategory.Caption
      					MySelectedAppointment..BackgroundColor	= OrganizerCategory.Color = -1 ? RGB(191, 210, 234) else OrganizerCategory.Color	
      
      				END
      				
      			ELSE
      
      				// Cancels the association
      				OrganizerEvent.CategoryID = 0
      				
      				// Applies the category and the color of the category by default
      				MySelectedAppointment..Category 	= ""
      				MySelectedAppointment..BackgroundColor	= RGB(191, 210, 234)
      			END
      			
      			// Saves the association
      			HModify(OrganizerEvent)
      		END		
      	END
