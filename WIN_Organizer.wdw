      // Summary: Synchronizes the deletions
      PROCEDURE SynchronizeEventDeletions()
      
      	ArrayOfArray is Structure
      		Elements is array of strings
      	END
      	
      	aaOutlookDeletions 	is ASSOCIATIVE ARRAY (*, *, wlInt) of ArrayOfArray 
      	aaNotesDeletions 	is ASSOCIATIVE ARRAY (*, *, wlInt) of ArrayOfArray 
      	aaGoogleDeletions	is ASSOCIATIVE ARRAY (*, *, wlInt) of ArrayOfArray 
      	AnArrayOfArray		is ArrayOfArray
      	
      	// Messages
      	WIN_SynchroInfo.Process(0, "Synchronizing the organizers...", "Gathering the deleted events...")
      	
      	// Disables the triggers temporarily
      	HDeactivateTrigger()
      	
      	// For each event flagged as 'Deleted'
      	FOR EACH OrganizerEvent WHERE OrganizerEvent.IsDeleted = True
      		
      		// If the event is linked to a Google calendar
      		IF OrganizerEvent.GoogleCalendarID <> 0 _AND_ OrganizerEvent.GoogleIdentifier <> "" THEN
      			
      			// Retrieves the list of events to delete
      			IF aaGoogleDeletions[OrganizerEvent.GoogleCalendarID] = Null THEN
      				VariableReset(AnArrayOfArray)
      			ELSE
      				AnArrayOfArray = aaGoogleDeletions[OrganizerEvent.GoogleCalendarID]
      			END
      			
      			// Adds the event to the list
      			Add(AnArrayOfArray:Elements, OrganizerEvent.GoogleIdentifier)
      			
      			// Keeps the new list
      			aaGoogleDeletions[OrganizerEvent.GoogleCalendarID] = AnArrayOfArray			
      		END
      			
      		// Erases the association
      		OrganizerEvent.GoogleCalendarID 		= 0
      		OrganizerEvent.GoogleIdentifier 	= ""
      		
      		// If the event is linked to a Notes calendar
      		IF OrganizerEvent.NotesCalendarID <> 0 _AND_ OrganizerEvent.NotesIdentifier <> "" THEN
      			
      			// Retrieves the list of events to delete
      			IF aaNotesDeletions[OrganizerEvent.NotesCalendarID] = Null THEN
      				VariableReset(AnArrayOfArray)
      			ELSE
      				AnArrayOfArray = aaNotesDeletions[OrganizerEvent.NotesCalendarID]
      			END
      			
      			// Adds the event to the list
      			Add(AnArrayOfArray:Elements, OrganizerEvent.NotesIdentifier)
      			
      			// Keeps the new list
      			aaNotesDeletions[OrganizerEvent.NotesCalendarID] = AnArrayOfArray		
      		END	
      		
      		// Erases the association
      		OrganizerEvent.NotesCalendarID 		= 0
      		OrganizerEvent.NotesIdentifier 	= ""	
      		
      		// If the event is linked to a Notes calendar
      		IF OrganizerEvent.OutlookCalendarID <> 0 _AND_ OrganizerEvent.OutlookIdentifier <> "" THEN
      			
      			// Retrieves the list of events to delete
      			IF aaOutlookDeletions[OrganizerEvent.OutlookCalendarID] = Null THEN
      				VariableReset(AnArrayOfArray)
      			ELSE
      				AnArrayOfArray = aaOutlookDeletions[OrganizerEvent.OutlookCalendarID]
      			END
      			
      			// Adds the event to the list
      			Add(AnArrayOfArray:Elements, OrganizerEvent.OutlookIdentifier)
      			
      			// Keeps the new list
      			aaOutlookDeletions[OrganizerEvent.OutlookCalendarID] = AnArrayOfArray				
      		END
      			
      		// Erases the association
      		OrganizerEvent.OutlookCalendarID 		= 0
      		OrganizerEvent.OutlookIdentifier 		= ""		
      		
      		// If no association
      		IF OrganizerEvent.OutlookCalendarID = 0 _AND_ OrganizerEvent.NotesCalendarID = 0 _AND_ OrganizerEvent.GoogleCalendarID = 0 THEN
      			HDelete(OrganizerEvent)
      		ELSE
      			
      			// Saves the modifications
      			HModify(OrganizerEvent)
      			
      			// Erases the history of this event
      			FOR EACH OrganizerEventHistory WHERE OrganizerEventID = OrganizerEvent.OrganizerEventID
      				HDelete(OrganizerEventHistory)
      			END
      		END
      	END
      	
      	// Re-enables the triggers
      	HActivateTrigger()
      	
      	// Messages
      	WIN_SynchroInfo.Process(0, "Synchronizing the organizers...", "Deleting events from the Outlook calendars...")
      	
      	// Performs the Outlook deletions
      	FOR EACH AnArrayOfArray, nOrganizerID of aaOutlookDeletions
      		OutlookDeleteMultipleEvents(nOrganizerID, AnArrayOfArray:Elements)
      	END
      	
      	// Messages
      	WIN_SynchroInfo.Process(0, "Synchronizing the organizers...", "Deleting events from the Lotus calendars...")
      	
      	// Performs the Lotus deletions
      	FOR EACH AnArrayOfArray, nOrganizerID of aaNotesDeletions
      		NotesDeleteMultipleEvents(nOrganizerID, AnArrayOfArray:Elements)	
      	END
      	
      	// Messages
      	WIN_SynchroInfo.Process(0, "Synchronizing the organizers...", "Deleting events from the Google calendars...")
      	
      	// Performs the Google deletions
      	FOR EACH AnArrayOfArray, nOrganizerID of aaGoogleDeletions
      		GoogleDeleteMultipleEvents(nOrganizerID, AnArrayOfArray:Elements)	
      	END
