     type_code : 9
     p_codes :
      -
        code : |1-
         
         nColorEnd 	is int
         nDarkColor	is int
         nColor		is int = MySource.Color
         iCat			is an Image
         	
         // Lightens the color
         nColorEnd 	= HSL(ColorHue(nColor), ColorSaturation(nColor), Min(100, ColorLightness(nColor) + 10))
         nDarkColor 	= HSL(ColorHue(nColor), Max(0, ColorSaturation(nColor) - 20), Max(0,   ColorLightness(nColor) - 20))
         
         iCat..Width = 16
         iCat..Height = 16
         
         // Start the drawing
         dStartDrawing(iCat, dErase + dWithOpacity)
         dRectangle(0, 0, 15, 15, LightMagenta, LightMagenta)
         
         // Border
         dLine( 2,  1, 13,  1, nDarkColor, 1)
         dLine( 2, 14, 13, 14, nDarkColor, 1)
         dLine( 1,  2,  1, 13, nDarkColor, 1)
         dLine(14,  2, 14, 13, nDarkColor, 1)
         
         // Gradient
         dRectangleGradient(2, 2, 14, 14, nColorEnd, nColor)
         
         IF nColor = -1 THEN
         	dLine(2, 2, 13, 13, DarkRed, 1)
         END
         
         // Backup
         COL_PictoColor = dSaveImageGIF(iCat, inMemory)
         
         // Automatically selects the category
         IF Position(ListCategories, COL_OrganizerCategory, 1, WholeWord) > 0 THEN
         	COL_Selection = True
         END
