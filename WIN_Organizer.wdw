      // Summary: Run at the end of event edition
      PROCEDURE OnEndEventEdition(MyAppointment is an Appointment)
      
      	// If we are in addition mode
      	IF ModeAppointmentEdition = AptAddition THEN
      		
      		// Check whether the appointment has a title
      		IF MyAppointment..Title = "" THEN
      			
      			// Searches for the appointment
      			HReadSeekFirst(OrganizerEvent, OrganizerEventID, MyAppointment..ID)
      			dbgAssert(HFound(OrganizerEvent))
      			
      			IF HFound(OrganizerEvent) THEN
      				
      				// Delete it
      				HDelete(OrganizerEvent)
      			END
      			
      			// Erases in the organizer
      			OrganizerDeleteAppointment(ORG_ORGANIZER, ORG_ORGANIZER)
      			MySelectedAppointment <- Null
      			RETURN
      		END		
      	END
      
      	// Searches for the event
      	HReadSeekFirst(OrganizerEvent, OrganizerEventID, MyAppointment..ID)
      	dbgAssert(HFound(OrganizerEvent))
      	
      	IF HFound(OrganizerEvent) THEN
      		
      		// Keeps the new title
      		OrganizerEvent.Title = MyAppointment..Title
      		
      		// Saves the modifications
      		HModify(OrganizerEvent)		
      	END
      	
      	// Keeps the "selected" event
      	OnEventSelection(MyAppointment)
      	
      END:
      	ModeAppointmentEdition = 0
     type : 458752
