     p_codes :
      -
        code : |1-
         
         DisplayEvents()
        type : 14
      -
        code : |1-
         
         // Manages the addition of an appointment manually
         // -> creates a record in the database and modifies the organizer event to associate it with the record in the database (ID property of ogiAppointment)
         IF ORG_ORGANIZER > 0 THEN OnEventAdd(ORG_ORGANIZER[ORG_ORGANIZER])
        type : 135
      -
        code : |1-
         
         // Manages the beginning of event edit (editing the title)
         // ->
         IF ORG_ORGANIZER > 0 THEN OnBeginningEventEdition(ORG_ORGANIZER[ORG_ORGANIZER])
        type : 136
      -
        code : |1-
         
         // Manages the end of manual event edit
         // -> saves the .Title modifications (only modifiable element during an edit)
         IF ORG_ORGANIZER > 0 THEN OnEndEventEdition(ORG_ORGANIZER[ORG_ORGANIZER])
        type : 137
      -
        code : |1-
         
         // Manages the selection of an event
         // ->
         IF ORG_ORGANIZER > 0 THEN OnEventSelection(ORG_ORGANIZER[ORG_ORGANIZER])
        type : 130
      -
        code : |1-
         
         // Manages the move of an event
         // -> saves the modifications of .StartDate and .EndDate
         IF ORG_ORGANIZER > 0 THEN OnEventMove(ORG_ORGANIZER[ORG_ORGANIZER])
        type : 131
      -
        code : |1-
         
         // Manages the resizing of an event
         // -> saves the modifications of .StartDate and .EndDate
         IF ORG_ORGANIZER > 0 THEN OnEventResizing(ORG_ORGANIZER[ORG_ORGANIZER])
        type : 132
      -
        code : |1-
         
         // Manages the manual deletion of an event
         // -> asks for confirmation before the deletion
         // -> flags the event as deleted in the database
         IF ORG_ORGANIZER > 0 THEN OnDeleteEvent(ORG_ORGANIZER[ORG_ORGANIZER])
        type : 134
      -
        code : |1-
         
         IF ModeAppointmentEdition <> 0 THEN	
         	// Ends the edit
         	OnEndEventEdition(MySelectedAppointment)
         END
         
         // No selected appointment
         MySelectedAppointment <- Null
         
         // Retrieve the period with ..StartDate and ..EndDate
         SelectedPeriod.StartDate 	= ORG_ORGANIZER..StartDate
         SelectedPeriod.EndDate 	= ORG_ORGANIZER..EndDate
        type : 133
      -
        code : |1-
         
         // 
         OnOrganizerZoom()
         
         // No selected appointment
         MySelectedAppointment <- Null
         
         dDate is date
         
         // Retrieves the current date
         dDate = OrganizerPosition(ORG_ORGANIZER)
         
         // Selects the date in the calendar
         CAL_CALENDAR = dDate
         CalendarPeriod(dDate)
         
         // Refreshes the events
         DisplayEvents()
        type : 138
      -
        code : |1-
         
        type : 66048
      -
        internal_properties : 
        code : |1-
         
         // The mouse coordinates are in 'Screen origin' mode
         nMouseXPos is int = MouseXPos
         nMouseYPos is int = MouseYPos
         
         // Start to give focus to the organizer
         OrganizerGiveFocus(ORG_ORGANIZER, nMouseXPos, nMouseYPos)
         
         // Retrieves the subscript of the hovered event
         nSubscript is int = OrganizerInfoXY(ORG_ORGANIZER, oriAppointment, nMouseXPos, nMouseYPos)
         IF nSubscript > 0 THEN
         		
         	AppointmentSubscript = nSubscript
         	
         	// Enables the 'Copy the appointment' option
         	MENU_Organizer.OPT_CopyAppointment..State = Active	
         	
         	// Enables the 'Export the appointment' option
         	MENU_Organizer.OPT_ExportAppointment..State = Active	
         	
         	// Enables the 'Classify' option
         	MENU_Organizer.OPT_ClassifyCategory..State = Active
         	
         	// If the appointment has no category
         	IF ORG_ORGANIZER[nSubscript].Category = "" THEN
         		
         		// Disables the 'Erase the category' option
         		MENU_Organizer.OPT_ClassifyCategory.OPT_EraseCategory..State = Grayed
         	ELSE
         		
         		// Enables the 'Erase the category' option
         		MENU_Organizer.OPT_ClassifyCategory.OPT_EraseCategory..State = Active
         	END
         	
         	// Enables the 'Modify the appointment' option
         	MENU_Organizer.OPT_ModifyAppointment..State = Active
         		
         	// Enables the 'Link to an organizer' option
         	MENU_Organizer.OPT_Link..State = Active
         	
         	// Searches for the appointment
         	HReadSeekFirst(OrganizerEvent, OrganizerEventID, ORG_ORGANIZER[nSubscript].ID)
         	IF HFound(OrganizerEvent) THEN
         		
         		// If the appointment is linked to an Outlook, Notes or Google calendar
         		MENU_Organizer.OPT_Link.OPT_LinkOutlook.OPT_LinkOutlook_Cancel..State 		= (OrganizerEvent.OutlookCalendarID 	<> 0 ? Active ELSE Grayed)
         		MENU_Organizer.OPT_Link.OPT_LinkLotus_Notes.OPT_LinkLotus_Cancel..State 	= (OrganizerEvent.NotesCalendarID 	<> 0 ? Active ELSE Grayed)
         		MENU_Organizer.OPT_Link.OPT_LinkGoogle.OPT_LinkGoogle_Cancel..State 		= (OrganizerEvent.GoogleCalendarID 	<> 0 ? Active ELSE Grayed)	
         		
         	END
         ELSE
         	
         	AppointmentSubscript = 0
         	
         	// Disables the 'Copy the appointment' option
         	MENU_Organizer.OPT_CopyAppointment..State = Grayed	
         	
         	// Disables the 'Export the appointment' option
         	MENU_Organizer.OPT_ExportAppointment..State = Grayed	
         	
         	// Disables the 'Classify' option
         	MENU_Organizer.OPT_ClassifyCategory..State = Grayed
         	
         	// Disables the 'Modify the appointment' option
         	MENU_Organizer.OPT_ModifyAppointment..State = Grayed
         	
         	// Disables the 'Link to an organizer' option
         	MENU_Organizer.OPT_Link..State = Grayed
         END
         
         // If an appointment was copied
         IF AppointmentClipboard <> Null THEN
         
         	// Enables the 'Paste the appointment' option
         	MENU_Organizer.OPT_PasteAppointment..State = Active
         ELSE
         
         	// Disables the 'Paste the appointment' option	
         	MENU_Organizer.OPT_PasteAppointment..State = Grayed
         END
