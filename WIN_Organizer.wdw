      // Summary: <specify the action of the procedure>
      PROCEDURE SynchronizeEventModifications()
      
      	ArrayOfArray is Structure
      		Elements is array of int
      	END
      	
      	aaOutlookModifs 	is ASSOCIATIVE ARRAY (*, *, wlInt) of ArrayOfArray 
      	aaNotesModifs 		is ASSOCIATIVE ARRAY (*, *, wlInt) of ArrayOfArray 
      	aaGoogleModifs		is ASSOCIATIVE ARRAY (*, *, wlInt) of ArrayOfArray 
      	AnArrayOfArray	is ArrayOfArray
      
      	// Messages
      	WIN_SynchroInfo.Process(2, "Synchronizing the organizers...", "Gathering the modified events...")
      
      	FOR EACH OrganizerEvent 
      		
      		// Search for an history for this event
      		HSeekFirst(OrganizerEventHistory, OrganizerEventID, OrganizerEvent.OrganizerEventID)
      		IF HFound(OrganizerEventHistory) THEN
      						
      			// If an history was found, these modifications must be updated
      			IF OrganizerEvent.GoogleCalendarID <> 0 _OR_ OrganizerEvent.NotesCalendarID <> 0 _OR_ OrganizerEvent.OutlookCalendarID <> 0 THEN
      				
      				// The event is linked to a Google calendar
      				IF OrganizerEvent.GoogleCalendarID <> 0 THEN
      					
      					// Retrieves the list of modifications
      					IF aaGoogleModifs[OrganizerEvent.GoogleCalendarID] = Null THEN
      						VariableReset(AnArrayOfArray)
      					ELSE
      						AnArrayOfArray = aaGoogleModifs[OrganizerEvent.GoogleCalendarID]
      					END
      					
      					// Adds the event to the list
      					Add(AnArrayOfArray:Elements, OrganizerEvent.OrganizerEventID)
      					
      					// Keeps the new list
      					aaGoogleModifs[OrganizerEvent.GoogleCalendarID] = AnArrayOfArray
      				END
      				
      				// The event is linked to an Outlook calendar
      				IF OrganizerEvent.OutlookCalendarID <> 0 THEN
      					
      					// Retrieves the list of modifications
      					IF aaOutlookModifs[OrganizerEvent.OutlookCalendarID] = Null THEN
      						VariableReset(AnArrayOfArray)
      					ELSE
      						AnArrayOfArray = aaOutlookModifs[OrganizerEvent.OutlookCalendarID]
      					END
      					
      					// Adds the event to the list
      					Add(AnArrayOfArray:Elements, OrganizerEvent.OrganizerEventID)
      					
      					// Keeps the new list
      					aaOutlookModifs[OrganizerEvent.OutlookCalendarID] = AnArrayOfArray
      				END
      				
      				// The event is linked to a Notes calendar
      				IF OrganizerEvent.NotesCalendarID <> 0 THEN
      					
      					// Retrieves the list of modifications
      					IF aaNotesModifs[OrganizerEvent.NotesCalendarID] = Null THEN
      						VariableReset(AnArrayOfArray)
      					ELSE
      						AnArrayOfArray = aaNotesModifs[OrganizerEvent.NotesCalendarID]
      					END
      					
      					// Adds the event to the list
      					Add(AnArrayOfArray:Elements, OrganizerEvent.OrganizerEventID)
      					
      					// Keeps the new list
      					aaNotesModifs[OrganizerEvent.NotesCalendarID] = AnArrayOfArray
      				END
      			END
      		END
      	END
      	
      	// Messages
      	WIN_SynchroInfo.Process(2, "Synchronizing the organizers...", "Modifying events in the Outlook calendars...")
      	
      	// Performs the Outlook modifications
      	FOR EACH AnArrayOfArray, nOrganizerID of aaOutlookModifs
      		OutlookModifyMultipleEvents(nOrganizerID, AnArrayOfArray:Elements)	
      	END
      
      	// Messages
      	WIN_SynchroInfo.Process(2, "Synchronizing the organizers...", "Modifying events in the Lotus calendars...")
      
      	// Performs the Notes modifications
      	FOR EACH AnArrayOfArray, nOrganizerID of aaNotesModifs
      		NotesModifyMultipleEvents(nOrganizerID, AnArrayOfArray:Elements)	
      	END
      
      	// Messages
      	WIN_SynchroInfo.Process(2, "Synchronizing the organizers...", "Modifying some events in the Google calendars...")
      
      	// Performs the Google modifications
      	FOR EACH AnArrayOfArray, nOrganizerID of aaGoogleModifs
      		GoogleModifyMultipleEvents(nOrganizerID, AnArrayOfArray:Elements)
      	END
      	
      	// Erases the history of events
      	HDeleteAll(OrganizerEventHistory)
