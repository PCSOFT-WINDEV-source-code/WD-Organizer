      // Summary: Retrieves the new events
      PROCEDURE ImportEvents()
      
      	nPosHF is int
      
      	// Opens the progress window
      	OpenChild(WIN_SynchroInfo)
      	
      	// Disable the DDW temporarily to avoid the flickering effects
      	// with the conflict window
      	WinActivateDDW(WIN_Organizer, False)
      	WinActivateDDW(WIN_SynchroInfo, False)
      	
      	// Bound of the progress bar
      	WIN_SynchroInfo.PROGBAR_PROCESS..MaxValue = HNbRec(OutlookCalendar) * 2 + HNbRec(NotesCalendar) * 2 + HNbRec(GoogleCalendar) * 2
      		
      	// Messages
      	WIN_SynchroInfo.Process(0, "Synchronizing the Outlook calendars...")
      
      	////////////////////////////////////////////////////
      	// Outlook calendars
      	//////////////////////////
      
      	FOR EACH OutlookCalendar 
      		
      		WIN_SynchroInfo.Process(1, "Synchronizing the Outlook calendars...", OutlookCalendar.Profile)
      					
      		// Save the context 
      		nPosHF = HSavePosition(OutlookCalendar)
      		
      		// Imports the new events
      		OutlookImportCalendar(OutlookCalendar.Profile, OutlookCalendar.OutlookCalendarID, False)
      		
      		WIN_SynchroInfo.Process(1)
      		OutlookCheckDeletions(OutlookCalendar.Profile, OutlookCalendar.OutlookCalendarID)
      		
      		// Restore the context
      		HRestorePosition(nPosHF)		
      	END
      	
      	WIN_SynchroInfo.Process(0, "Synchronizing the Google calendars...")
      	
      	////////////////////////////////////////////////////
      	// Google calendars
      	//////////////////////////
      
      	FOR EACH GoogleCalendar 
      		
      		WIN_SynchroInfo.Process(1, "Synchronizing the Google calendars...", GoogleCalendar.Name)
      		
      		// Save the context 
      		nPosHF = HSavePosition(GoogleCalendar)
      				
      		MyConnection is a gglConnection
      		MyConnection = GoogleConnectCalendar()
      		
      		// Imports the new events
      		GoogleImportCalendar(MyConnection, GoogleCalendar.Identifier, GoogleCalendar.GoogleCalendarID, False)
      		
      		WIN_SynchroInfo.Process(1)
      		GoogleCheckDeletions(MyConnection, GoogleCalendar.Identifier, GoogleCalendar.GoogleCalendarID)
      		
      		// Restore the context
      		HRestorePosition(nPosHF)		
      	END
      	
      	WIN_SynchroInfo.Process(0, "Synchronizing the Notes calendars...")
      	
      	////////////////////////////////////////////////////
      	// Notes Organizers
      	//////////////////////////
      	
      	FOR EACH NotesCalendar 
      		
      		WIN_SynchroInfo.Process(1, "Synchronizing the Notes calendars...", NotesCalendar.Name)
      		
      		// Save the context 
      		nPosHF = HSavePosition(NotesCalendar)
      		
      		// Imports the new events
      		NotesImportCalendar(NotesCalendar.NotesCalendarID, False)
      		
      		WIN_SynchroInfo.Process(1)
      		NotesCheckDeletions(NotesCalendar.NotesCalendarID)
      		
      		// Restore the context
      		HRestorePosition(nPosHF)		
      	END
      	
      	Close(WIN_SynchroInfo)
      	
      	// Re-enables the DDW
      	WinActivateDDW(WIN_Organizer, True)
     type : 458752
