   -
     name : DisplayEvents
     internal_properties : CAAAAAgAAAAwuIGZvN8ADa6JZmxZsqV+E5YBn42tWZfMrELGNfMkZY5Gfh6/0iKagoDzrAgzQbcAyd8CN4XvFg8iRL7aisBG//ussfIlqyvs20sDDWuxvUyH1ENDxrPAQm9qsOPstt99tMNpkjX5ImTfyo0c9hW0WznNdJKLbgX8scB+ZdWTgmGzU+GSBygF+ZZ4sHUR0Imt3HXQI8+hSQz7wWXirVnoCibWTsbT
     procedure_id : 1760073861357892252
     type_code : 14
     code : |1-
      // Summary: Displays the events of the period currently displayed
      PROCEDURE DisplayEvents()
      
      	AnAppointment is an Appointment
      	dtStartDate	 is datetime
      	dtEndDate	 is datetime
      
      	HourGlass(True)
      
      	// Clears the organizer
      	OrganizerDeleteAll(ORG_ORGANIZER)
      	
       	// Retrieves the selected period
       	dtStartDate = OrganizerPosition(ORG_ORGANIZER)
       	dtEndDate	= OrganizerPosition(ORG_ORGANIZER, orgEndDate)
      	
      	// Configure the query
      	QRY_Events.pStartDate 		= dtStartDate
      	QRY_Events.pEndDate   		= dtEndDate
      	QRY_Events.pListCategories	= (ListCategories = "" ? Null ELSE ListCategories)
      	
      	// Run the query
      	HExecuteQuery(QRY_Events)
      	IF ErrorOccurred THEN Error()
      
      	FOR EACH QRY_Events 
      				
      		// Copies the appointment data
      		AnAppointment..Author 					= QRY_Events.Author
      		AnAppointment..Content					= QRY_Events.Content
      		AnAppointment..StartDate					= QRY_Events.StartDate
      		AnAppointment..EndDate					= QRY_Events.EndDate
      		AnAppointment..ID						= QRY_Events.OrganizerEventID
      		AnAppointment..Guest					= QRY_Events.Guests
      		AnAppointment..Location						= QRY_Events.Locations
      		AnAppointment..Title						= QRY_Events.Title
      		AnAppointment..WholeDay			= QRY_Events.WholeDay
      		AnAppointment..Category					= QRY_Events.CategoryCaption
      		AnAppointment..WithRepetition			= (QRY_Events.RepetitionType <> -1)
      		AnAppointment..BackgroundColor				= (QRY_Events.CategoryCaption = "" _or_ QRY_Events.Color < 0 ? RGB(191, 210, 234) else QRY_Events.Color)
      		
      		// Recurrence of the event
      		IF AnAppointment..WithRepetition THEN
      			AnAppointment..Repetition..Type		  	= QRY_Events.RepetitionType
      			AnAppointment..Repetition..TypeEnd	  	= QRY_Events.RepetitionEndType			
      			AnAppointment..Repetition..NbOccurrence 	= QRY_Events.RepetitionOccurrence
      			AnAppointment..Repetition..EndDate	  	= QRY_Events.RepetitionEndDate
      		END
      		
      		AnAppointment..Image = ""
      						
      		// Exclusive Google Calendar event
      		IF QRY_Events.GoogleCalendarID <> 0 THEN AnAppointment..Image += [CR] + IMG_GOOGLEPIC
      				
      		// Exclusive Outlook event
      		IF QRY_Events.OutlookCalendarID <> 0 THEN AnAppointment..Image += [CR] +  IMG_OUTLOOKPIC
      				
      		// Exclusive Lotus Notes event
      		IF QRY_Events.NotesCalendarID <> 0 THEN AnAppointment..Image += [CR] +  IMG_NOTESPIC							
      		
      		// Adds the appointment
      		OrganizerAddAppointment(ORG_ORGANIZER, AnAppointment)		
      	END
      	
      	HourGlass(False)
