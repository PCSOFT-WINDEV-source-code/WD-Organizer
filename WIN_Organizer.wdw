         -
           code : |1-
            
            sCAL 		is string ANSI
            sFile 	is string
            
            // Opens the file picker
            sFile = fSelect("", "", "Import the appointments from...", "iCalendar (*.ICS)" + TAB + "*.ICS", "*.ICS", fselOpen)
            
            IF sFile <> "" THEN
            
            	// Loads the content
            	sCAL = fLoadText(sFile)
            
            	// Retrieves the appointments
            	arrApt is array of Appointment = iCalendarToAppointment(sCAL)
            	
            	// Transfers the events to the organizer
            	FOR EACH anApt of arrApt
