      // Sumary: Prepares the popup menu
      PROCEDURE PreparePopupMenu()
      
      	nSubscript is int
      	sOptName	is string
      
      	// Grays the clipboard options
      	OPT_PasteAppointment..State = (AppointmentClipboard = Null ? Grayed ELSE Active)
      	OPT_CopyAppointment..State = (MySelectedAppointment = Null ? Grayed ELSE Active)
      				
      	// Creates the menu options for the categories
      	FOR EACH OrganizerCategory FromEnd
      		
      		nSubscript ++
      		
      		// Creates the name of the option
      		sOptName = "OPT_CAT" + OrganizerCategory.OrganizerCategory
      		
      		// If the option already exists
      		IF MenuExist(OPT_ClassifyCategory, sOptName) THEN
      		
      			WHEN EXCEPTION IN 
      				MenuDelete(sOptName)
      			DO
      				//
      			END
      		END
      			
      		// Creates the menu option
      		MenuInsertOption(OPT_ClassifyCategory, OPT_SeparatorCategories, sOptName, OrganizerCategory.Caption, OnEventClassification, OrganizerCategory.OrganizerCategory)				
      		
      		// Modifies the image of the option
      		{OPT_ClassifyCategory..FullName + "." + sOptName, indControl}..Image = CreateCategoryImage(sOptName, OrganizerCategory.Color)
      		
      		// Display 5 elements in the menu only
      		IF nSubscript > 5 THEN
      			BREAK
      		END
      	END
      	
      	nSubscript = 0
      	
      	// Creates the menu options for the external Outlook calendars
      	FOR EACH OutlookCalendar 
      		
      		// Creates the name of the option
      		sOptName = "OPT_OUTLOOK_" + OutlookCalendar.OutlookCalendarID
      		nSubscript ++
      		
      		WHEN EXCEPTION IN 
      			MenuDelete(sOptName)
      		DO
      			//
      		END
      		
      		// Creates the menu option		
      		MenuInsertOption(OPT_LinkOutlook, OPT_Outlook_Sep, sOptName, OutlookCalendar.Name, OnLinkOutlookEvent, OutlookCalendar.OutlookCalendarID)	
      	END
      		
      	MENU_Organizer.OPT_Link.OPT_LinkOutlook.OPT_Outlook_Sep..Visible = (nSubscript > 0)
      	nSubscript = 0
      	
      	// Creates the menu options for the external Notes calendars
      	FOR EACH NotesCalendar 
      		
      		// Creates the name of the option
      		sOptName = "OPT_NOTES_" + NotesCalendar.NotesCalendarID
      		nSubscript ++
      		
      		WHEN EXCEPTION IN 
      			MenuDelete(sOptName)
      		DO
      			//
      		END
      		
      		// Creates the menu option
      		MenuInsertOption(OPT_LinkLotus_Notes, OPT_LinkLotus_Sep, sOptName, NotesCalendar.Name, OnLinkNotesEvent, NotesCalendar.NotesCalendarID)	
      	END
      	
      	MENU_Organizer.OPT_Link.OPT_LinkLotus_Notes.OPT_LinkLotus_Sep..Visible = (nSubscript > 0)
      	nSubscript = 0
      	
      	// Creates the menu options for the external Google calendars
      	FOR EACH GoogleCalendar
      		
      		// Creates the name of the option
      		sOptName = "OPT_GOOGLE_" + GoogleCalendar.GoogleCalendarID
      		nSubscript ++
      		
      		WHEN EXCEPTION IN 
      			MenuDelete(sOptName)
      		DO
      			//
      		END
      		
      		// Creates the menu option
      		MenuInsertOption(OPT_LinkGoogle, OPT_LinkGoogle_Sep, sOptName, GoogleCalendar.Name, OnLinkGoogleEvent, GoogleCalendar.GoogleCalendarID)	
      	END
      	
      	MENU_Organizer.OPT_Link.OPT_LinkGoogle.OPT_LinkGoogle_Sep..Visible = (nSubscript > 0)
