         -
           internal_properties : CAAAAAgAAAAjKuIsl2VC7bGyw6UOjuaritQ6cE2Twp2zFLqYdAundyQh9ZWqCYerDOuMycsmfW4DH/XbzscshxKv48gJJcvW9Eg+5Cu49vysVSNmFuqlPFqHEnpkWOZ51AWkfNCGWbNcgOcrJaC072EDryQB9G7aUQiERDb7mOgh1k43E4Wsrw==
           code : |1-
            
            	// If an appointment is accessible
            	IF AppointmentSubscript > 0 THEN
            		
            		// Stores the appointment
            		AppointmentClipboard <- ORG_ORGANIZER[AppointmentSubscript]		
            
            		
            		// Enables the 'Paste the appointment' option
            		MENU_Organizer.OPT_PasteAppointment..State = Active
            	ELSE
            		
            		AppointmentClipboard <- Null
            		
            		// Disables the 'Paste the appointment' option
            		MENU_Organizer.OPT_PasteAppointment..State = Grayed
            	END
            	
            	AppointmentSubscript = 0
           type : 32
     -
       name : OPT_PasteAppointment
       identifier : 0x286d09da304bf2bb
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           internal_properties : CAAAAAgAAACromqk/1q0+ZuISbsUgJbFljh6iMug2omfTFKIFAPXbwzhxW1KSe/7tNu80ZOmBY5jl41bVu+ML5LnW6h57fveGHTimCdcmshAsQ9iEuaZoF4Tth4QXFqFKKHw2Hzy3ccQTCufcdRIF3XvSlHsAW870gmLDR9yc/Oef+a/mw0kJw==
           code : |1-
            
            	AnAppointment is an Appointment
            	tTime 		 is time = Now
            	dDate		 is date  = OrganizerPosition(ORG_ORGANIZER)
            	dtDateTime	 is datetime
            	nAptSubscript	 is int
            	duDuration		 is duration
            	
            	//
            	// Pastes the appointment that was previously copied into the organizer.
            	//
            	// - If a period was selected, redefines the beginning and the end of the copied event with this period
            	// or
            	// - If another appointment was selected, redefine the beginning of the copied event with the beginning of the selected event. Its duration is unchanged.
            	// or
            	// - If no other appointment was selected AND if no period was selected, paste the appointment at the current date and at the current time. Its duration is unchanged.
            	//
            
            	// If an appointment was copied
            	IF AppointmentClipboard <> Null THEN
            		
            		// Copies the appointment
            		AnAppointment = AppointmentClipboard
            		
            		// Erases the ID and the pictograms
            		AnAppointment..ID = ""
            		AnAppointment..Image = ""
            		
            		// Calculates the duration of the appointment
            		duDuration = AnAppointment..EndDate - AnAppointment..StartDate
            				
            		// If no period is selected
            		IF ORG_ORGANIZER..StartDate = "" _AND_ ORG_ORGANIZER..EndDate = "" THEN
            			
            			// If not other appointment is selected
            			IF MySelectedAppointment = Null THEN
            					
            				// Erases the seconds/milliseconds
            				tTime..Millisecond = 0
            				tTime..Second 	 = 0
            				
            				// Between xxH00 and xxH29, use the hour
            				// ex.: 14:08 -> 14:00
            				IF tTime..Minute < 30 THEN
            					tTime..Minute = 0
            					
            				// Between xxH30 and xxH59, use the hour and half
            				// ex.: 14:45 -> 14:30
            				ELSE
            					tTime..Minute = 30
            				END
            				
            				// Creates a DateTime
            				dtDateTime = dDate + tTime
            				
            				// Modifies the date of the appointment
            				AnAppointment..StartDate = dtDateTime
            				
            				// Applies the initial duration
            				dtDateTime += duDuration
            				AnAppointment..EndDate   = dtDateTime
            			ELSE
            							
            				// Give the same start date as the selected appointment
            				AnAppointment..StartDate = MySelectedAppointment..StartDate
            				
            				// Calculates the end date from the initial duration of the appointment
            				dtDateTime = AnAppointment..StartDate
            				dtDateTime += duDuration
            				
            				// Applies the end date
            				AnAppointment..EndDate   = dtDateTime			
            			END
            		ELSE
            		
            			// Applies the selected period to the appointment
            			AnAppointment..StartDate = ORG_ORGANIZER..StartDate
            			AnAppointment..EndDate   = ORG_ORGANIZER..EndDate
            			
            			// Re-initializes the selected period
            			ORG_ORGANIZER..StartDate = ""
            			ORG_ORGANIZER..EndDate = ""
            		END
            		
            		// Adds the appointment
            		nAptSubscript = OrganizerAddAppointment(ORG_ORGANIZER, AnAppointment)
            		
            		// Starts the 'Adding an appointment' process
            		OnEventAdd(ORG_ORGANIZER[nAptSubscript])
            	END
           type : 32
